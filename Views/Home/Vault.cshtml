@model Dictionary<string, string>
@{
  ViewData["Title"] = "Vault Page";
}

<h4>Vault Page</h4>
<table class="table table-bordered table-responsive-sm table-hover table-striped col-6">
  <tr>
    <th>Id</th>
    <th>Value</th>
  </tr>
  @foreach (var item in Model)
  {
    <tr>
      <td>@item.Key</td>
      <td>@item.Value</td>
    </tr>
  }

</table>